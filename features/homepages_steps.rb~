When /^I go to the application root$/ do
  visit root_path
end

Then /^I should see "(.*?)"$/ do |message|
  page.should have_content message
end

When /^I go to About$/ do
  visit about_path
end

When /^I go to Privacy$/ do
  visit privacy_path
end

When /^I go to Contact$/ do
  visit contact_path
end



